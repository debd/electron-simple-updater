{
  "name": "electron-simple-updater",
  "version": "1.5.0",
  "description": "Simple way to enable update for the electron application",
  "main": "index.js",
  "scripts": {
    "test": "npm run lint && npm run tslint && npm run mocha",
    "mocha": "mocha 'spec/mocks/*.mock.js' 'spec/**/*.js'",
    "lint": "eslint '**/*.js' --ignore-pattern 'node_modules'",
    "tslint": "tslint -p . -t verbose index.d.ts"
  },
  "typings": "index.d.ts",
  "repository": "megahertz/electron-simple-updater",
  "files": [
    "index.js",
    "index.d.ts",
    "lib/*"
  ],
  "keywords": [
    "electron",
    "atom",
    "updater",
    "update",
    "publish",
    "release",
    "windows",
    "mac",
    "osx",
    "linux",
    "desktop"
  ],
  "author": "Alexey Prokhorov",
  "license": "MIT",
  "bugs": "https://github.com/megahertz/electron-simple-updater/issues",
  "homepage": "https://github.com/megahertz/electron-simple-updater#readme",
  "engines": {
    "node": ">=8.0"
  },
  "devDependencies": {
    "chai": "*",
    "electron": "*",
    "eslint": "*",
    "eslint-config-airbnb-base": "*",
    "eslint-plugin-import": "*",
    "mocha": "*",
    "mock-require": "*",
    "sinon": "*",
    "sinon-chai": "*",
    "tslint": "^5.14.0",
    "typescript": "^3.3.4000"
  },
  "dependencies": {
    "httpreq": "debd/node-httpreq",
    "semver": "*",
    "url-parse": "^1.4.7"
  }
}
